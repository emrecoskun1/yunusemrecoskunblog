{
  "name": "yunus-emre-coskun-portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Yunus Emre Coşkun - Fullstack Developer Portfolio",
  "keywords": [
    "portfolio",
    "astro",
    "fullstack",
    "developer",
    "yunus-emre-coskun"
  ],
  "author": "Yunus Emre Coşkun <yunusemrecoskun@outlook.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.14.4",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "dev": "astro dev",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "lint-staged": {
    "./**/*.{js,mjs,cjs}": [
      "eslint --cache --fix --ext .js,.mjs,.cjs ./",
      "prettier --cache --write \"./**/*.{js,mjs,cjs}\""
    ],
    "./**/*.ts": [
      "eslint --cache --fix --ext .ts ./",
      "prettier --cache --write \"./**/*.ts\""
    ],
    "./src/**/*.astro": [
      "eslint --cache --fix --ext .astro ./src/",
      "prettier --cache --write \"./src/**/*.astro\""
    ],
    "./src/**/*.css": "prettier --cache --write \"./src/**/*.css\"",
    "./**/*.json": "prettier --cache --write \"./**/*.json\"",
    "./**/*.yml": "prettier --cache --write \"./**/*.yml\""
  },
  "dependencies": {
    "@astrolicious/i18n": "0.6.1",
    "@fontsource-variable/inter": "5.1.1",
    "@playform/compress": "0.1.7",
    "astro-compressor": "1.0.0",
    "astro-icon": "1.1.5",
    "astro-seo-schema": "5.0.0",
    "astro-sitemap": "1.0.0",
    "astro-tunnel": "0.1.6",
    "i18next": "^23.0.0",
    "sanitize.css": "13.0.0",
    "schema-dts": "1.1.2",
    "typescript": "5.7.3"
  },
  "devDependencies": {
    "@astrojs/check": "0.9.4",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.0",
    "astro": "4.15.12",
    "astro-eslint-parser": "1.2.1",
    "eslint": "8.57.0",
    "eslint-config-love": "47.0.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "husky": "9.1.7",
    "lint-staged": "15.4.3",
    "prettier": "3.4.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.6"
  }
}
